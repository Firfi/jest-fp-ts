{
  "name": "@relmify/jest-fp-ts",
  "version": "1.1.0",
  "description": "Custom Jest matchers for projects using using fp-ts and io-ts",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "files": [
    "/dist",
    "/types",
    "!**/*.map"
  ],
  "keywords": [
    "jest",
    "matchers",
    "fp-ts",
    "io-ts",
    "typescript",
    "functional"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/relmify/jest-fp-ts.git"
  },
  "author": "Leila Pearson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/relmify/jest-fp-ts/issues"
  },
  "homepage": "https://github.com/relmify/jest-fp-ts#readme",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -watch",
    "clean": "npm-run-all clean:**",
    "clean:dist": "rimraf dist",
    "clean:jest-cache": "jest --clear-cache --bail 0",
    "lint:check": "eslint ./src --ext .ts",
    "lint:fix": "eslint --fix ./src --ext .ts",
    "prettier:check": "prettier --check \"src/**/*.ts\"",
    "prettier:fix": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run clean && npm run lint:check && npm run prettier:check && npm ci && npm test && npm run build"
  },
  "dependencies": {
    "expect": "^26.1.0",
    "jest-get-type": "^26.0.0",
    "jest-matcher-utils": "^26.1.0"
  },
  "peerDependencies": {
    "fp-ts": "2.x",
    "io-ts": "2.x"
  },
  "devDependencies": {
    "@types/jest": "^26.0.5",
    "@types/node": "^14.0.24",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fp-ts": "^2.0.0",
    "io-ts": "^2.0.0",
    "jest": "^26.1.0",
    "jest-extended": "^0.11.5",
    "jest-watch-toggle-config": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
